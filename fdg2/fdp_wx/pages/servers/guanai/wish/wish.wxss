page{
  width: 100%;
  height: 100%;
  background-color: #f4f6f8;
}
.wish_nav{
  padding-top: 30rpx;
  padding-bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}
.wish_nav_li,.wish_nav_li_hover{
    flex: 1;  
    text-align: center;
    line-height: 83rpx;
    font-size: 34rpx;
    height: 80rpx;
    border-bottom: 6rpx solid #fff;
}
.wish_nav_li_hover{
  border-bottom: 6rpx solid #d9001b;
  color: #d9001b;
}
.wish_list{
  margin-top: 20rpx;
}
.wish_li{
  padding: 30rpx;
  background-color: white;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 25rpx;
  margin-top: 20rpx;
}
.wish_txt{
  font-size: 34rpx;
}
.wish_time,.wish_who{
  font-size: 26rpx;
  color: #555;
}
.wish_who,.wish_end{
  margin-top: 30rpx;
}
.login{
  position: fixed;
  bottom: 30rpx;
  width: 100%;
  
}