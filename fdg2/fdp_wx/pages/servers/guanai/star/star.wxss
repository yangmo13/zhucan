page {
  width: 100%;
  height: 100%;
  background-color: var(--gray);
}

.star {
  width: 100%;
  height: 100%;

}
.star_box{
  padding: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
}
.star_li{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  background-color: white;
}
.star_top{
  width: 230rpx;
  height: 230rpx;

}
.star_top image{
  width: 100%;
  height: 100%;
}
.star_li{
  width: 330rpx;
  height: 330rpx;
  margin-top: 20rpx;
}
.star_mes{
  font-size: 30rpx;
  color: #333;
  margin-top: 15rpx;
}
.isload{
  width: 100%;

  transform: .3s;
  position: relative;
  padding: 50rpx;
}