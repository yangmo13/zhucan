page{
  width: 100%;
  height: 100%;
}
.vedio_window{
  width: 100%;
  height: 360rpx;
}
.audio_reduce{
  padding:  30rpx 20rpx;
}
.reduce_name_box{
  font-size: 34rpx;
  color: #333;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.reduce_date{
  font-size: 28rpx;
  color: #999;
}
.reduce_label_box{
  padding: 20rpx 0;
  display: flex;
  justify-content: start;
  align-items: center;
}
.reduce_label{
  padding: 5rpx 15rpx;
  font-size: 28rpx;
  color: #999;
  background-color: var(--gray);
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.reduce_txt{
  font-size: 28rpx ;
  color: #333;
  
}